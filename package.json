{
  "name": "spotify-downloader-app",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start -c --dev-client",
    "run:android:dev": "ENVFILE=.env.dev react-native run-android --variant devDebug --appId com.ramzarzexpo.dev --main-activity com.ramzarzexpo.MainActivity",
    "run:android:prod": "expo run:android",
    "run:ios": "expo run:ios",
    "run:ios:release": "expo run:ios --configuration Release",
    "web": "expo start --web",
    "build:android:dev": "cd android && ./gradlew :app:assembleDebug",
    "build:android:prod": "cd android && ./gradlew :app:assembleRelease",
    "build:android:all": "yarn build:android:dev && yarn build:android:prod",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.5.0",
    "base-64": "^1.0.0",
    "expo": "49.0.8",
    "expo-auth-session": "^5.0.2",
    "expo-splash-screen": "0.20.5",
    "expo-status-bar": "1.6.0",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-svg": "^13.13.0",
    "react-redux": "^8.1.2",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "spotify-web-api-js": "^1.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
